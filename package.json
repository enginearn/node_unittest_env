{
  "name": "unittest_env",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "ts-node": "npx ts-node",
    "start": "npx ts-node src/index.ts",
    "build": "npx tsc",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --fix --ext .ts src",
    "prettier": "prettier --write \"src/**/*.ts\"",
    "prettier:check": "prettier --check \"src/**/*.ts\"",
    "test": "npx jest --silent=true --coverage --coverageReporters=text-lcov | npx coveralls",
    "test:watch": "npx jest --watch",
    "test:debug": "npx jest --runInBand --debug",
    "test-only": "npx jest --silent=true -o",
    "test:coverage": "npx jest --silent=true --coverage --coverageReporters=text-lcov | npx coveralls"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
